{
  "name": "stockmarketsimulator",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "backend": "bun run src/server/server.ts",
    "frontend": "vite",
    "dev": "concurrently \"bun run backend\" \"bun run frontend\"",
    "build": "bun build src/client/index.tsx --outdir=dist --splitting",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf ./dist && bun pm cache clean"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "vite": "^6.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.9.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "classnames": "^2.5.1",
    "framer-motion": "^12.4.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "recharts": "^2.15.1"
  }
}